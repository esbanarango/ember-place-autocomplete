{{#if (has-block)}}
  {{yield this}}
{{else}}
  <input
    value={{value}}
    name={{name}}
    tabindex={{tabindex}}
    disabled={{disabled}}
    onblur={{action "onBlur" value="target.value"}}
    oninput={{action (mut value) value="target.value"}}
    placeholder={{placeholder}}
    class={{inputClass}}
  >
{{/if}}
